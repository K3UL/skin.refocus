			<visible>SubString(Container.FolderPath,videodb://1/,left) + !stringcompare(Container.FolderPath,videodb://1/)</visible>
		</control>
		<!--movies videodb new-->
		<control type="label">
			<label>$LOCALIZE[31926]</label>
			<include>PageLabel</include>
			<visible>Substring(Container.FolderPath,videodb://4/,Left)</visible>
		</control>
		<!--movies trailers-->
		<control type="label">
			<label>$LOCALIZE[20338] [COLOR=Color2]$LOCALIZE[31011][/COLOR]</label>
			<include>PageLabel</include>
			<visible>substring(container.folderpath,plugin://plugin.video.apple,left)</visible>
		</control>
		<!--tvshows videodb-->
		<control type="label">
			<label>$LOCALIZE[20343] [COLOR=Color2]$INFO[Container.FolderName][/COLOR]</label>
			<include>PageLabel</include>
			<visible>SubString(Container.FolderPath,videodb://2/,left) + !stringcompare(Container.FolderPath,videodb://2/) + !Container.Content(seasons) + !Container.Content(episodes)</visible>
		</control>
		<!--seasons-->
		<control type="label">
			<label>$INFO[ListItem.TVShowTitle] [COLOR=Color2]$LOCALIZE[33054][/COLOR]</label>
			<include>PageLabel</include>
			<visible>Container.Content(seasons)</visible>
		</control>
		<!--episodes videodb-->
