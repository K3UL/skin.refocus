<includes>
	<include name="PageLabel">
		<posx>34</posx>
		<posy>20</posy>
		<width>950</width>
		<aligny>center</aligny>
		<font>pagelabel</font>
		<textcolor>Color1</textcolor>
		<include>transition_PageLabel</include>
		<animation effect="slide" end="0,-3" condition="Skin.HasSetting(numitems.enable)">Conditional</animation>
		<animation effect="slide" end="25" condition="Skin.HasSetting(indicator.enable) + Control.IsVisible(6100)">Conditional</animation>
		<visible>!Window.IsActive(busydialog) + !Control.IsVisible(9900) + ![Window.IsActive(movieinformation) | Window.IsActive(musicinformation)] + !Control.IsVisible(531)</visible>
	</include>
	<include name="PageLabelInfo">
		<posx>34</posx>
		<posy>20</posy>
		<width>950</width>
		<aligny>center</aligny>
		<font>pagelabel</font>
		<textcolor>Color1</textcolor>
		<include>transition_PageLabel</include>
		<animation effect="slide" end="0,-3" condition="Skin.HasSetting(numitems.enable)">Conditional</animation>
		<animation effect="slide" end="25" condition="Skin.HasSetting(indicator.enable) + Control.IsVisible(6100)">Conditional</animation>
		<visible>!Window.IsActive(busydialog) + !Control.IsVisible(9900)</visible>
	</include>
	
<!---VIDEO------>			
	<include name="pagelabels_Video">
	
	<!---sources--->
		<control type="label">
			<description>sources</description>
			<label>$LOCALIZE[157] [COLOR=Color2]$INFO[Container.FolderName][/COLOR]</label>
			<visible>Container.Content(Files)</visible>
			<include>PageLabel</include>
		</control>
		<!--movies sources-->
		<control type="label">
			<label>$LOCALIZE[157] [COLOR=Color2]$INFO[Container.FolderName][/COLOR]</label>
			<include>PageLabel</include>
			<visible>Container.Content(movies) + ![SubString(Container.FolderPath,videodb://,left) | SubString(Container.FolderPath,special://profile/playlists/video/,left) | SubString(Container.FolderPath,plugin,left)]</visible>
		</control>
		<!--tvshows sources-->
		<control type="label">
			<label>$LOCALIZE[157] [COLOR=Color2]$INFO[Container.FolderName][/COLOR]</label>
			<include>PageLabel</include>
			<visible>Container.Content(tvshows) + ![SubString(Container.FolderPath,videodb://,left) | SubString(Container.FolderPath,special://profile/playlists/video/,left)]</visible>
		</control>
		<!--music video sources-->
		<control type="label">
			<label>$LOCALIZE[157] [COLOR=Color2]$INFO[Container.FolderName][/COLOR]</label>
			<include>PageLabel</include>
			<visible>Container.Content(musicvideos) + ![SubString(Container.FolderPath,videodb://,left) | SubString(Container.FolderPath,special://profile/playlists/video/,left)]</visible>
		</control>

	<!---library--->
		<!--movies node-->
		<control type="label">
			<label>$LOCALIZE[342]</label>
			<include>PageLabel</include>
			<visible>stringcompare(Container.FolderPath,videodb://1/)</visible>
		</control>
		<!--tv shows node-->
		<control type="label">
			<label>$LOCALIZE[20343]</label>
			<include>PageLabel</include>
