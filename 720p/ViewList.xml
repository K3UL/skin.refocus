				<posx>40</posx>
				<width>500</width>
				<label>$INFO[ListItem.Label]</label>
				<include>list_Label-single</include>
			</control>
		</itemlayout>
		<focusedlayout condition="IsEmpty(Container.FolderPath) | ![stringcompare(Container.FolderPath,addons://sources/video/) | Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos)]" width="625" height="40">
