<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<include name="view_List-Data.Single">
		<param name="TextColor" default="Color1"/>
		<definition>
			<control type="label">
				<left>15</left>
				<width>595</width>
				<label>$INFO[ListItem.Label]</label>
				<height>38</height>
				<font>regular_Big</font>
				<aligny>center</aligny>
				<textcolor>$PARAM[TextColor]</textcolor>
				<selectedcolor>Selected</selectedcolor>
				<scroll>false</scroll>
			</control>
		</definition>
	</include>

	<include name="view_List-Data">
		<param name="TextLeft" default="15"/>
		<param name="TextColor" default="Color1"/>
		<param name="TextColor2" default="Color2"/>
		<param name="TextWidth" default="550"/>
		<definition>
			<control type="group">
				<left>$PARAM[TextLeft]</left>
				<control type="label">
					<top>6</top>
					<width>$PARAM[TextWidth]</width>
					<label>$VAR[media:Title]</label>
					<height>28</height>
					<font>regular_Bigger</font>
					<textcolor>$PARAM[TextColor]</textcolor>
					<selectedcolor>$PARAM[TextColor]</selectedcolor>
				</control>
				<control type="label">
					<top>28</top>
					<width>$PARAM[TextWidth]</width>
					<label>$VAR[media:Info] $VAR[rating:Song]</label>
					<height>40</height>
					<font>regular_Small-U</font>
					<textcolor>$PARAM[TextColor2]</textcolor>
					<selectedcolor>$PARAM[TextColor]</selectedcolor>
				</control>
				<control type="textbox">
					<top>66</top>
					<width>$PARAM[TextWidth]</width>
					<height>59</height>
					<label>$VAR[media:Plot]</label>
					<textcolor>$PARAM[TextColor]</textcolor>
					<selectedcolor>$PARAM[TextColor]</selectedcolor>
					<visible>Control.IsVisible(51)</visible>
				</control>
			</control>
		</definition>
	</include>

	<include name="view_List-Icon">
		<control type="group">
			<left>5</left>
			<top>5</top>
			<control type="image">	
				<width>57</width>
				<height>57</height>
				<texture>thumbs/default_Square.png</texture>
			</control>
			<control type="image">
				<width>57</width>
				<height>57</height>
				<texture fallback="DefaultIconTV.png">$INFO[ListItem.Art(thumb)]</texture>
			</control>
		</control>
	</include>

	<include name="view_List-Art">
		<param name="FrameWidth" default="245"/>
		<param name="FrameHeight" default="144"/>
		<param name="ArtWidth" default="231"/>
		<param name="ArtHeight" default="130"/>
		<param name="ArtFallback" default="thumbs/default_Landscape.png"/>
		<param name="ArtType" default="$VAR[art:Landscape]"/>
		<param name="ShowLogo" default="true"/>
		<definition>
			<control type="image">
				<width>$PARAM[FrameWidth]</width>
				<height>$PARAM[FrameHeight]</height>
				<include>art_Frame-nf</include>
			</control>
			<control type="group">
				<left>7</left>
				<top>7</top>
				<width>$PARAM[ArtWidth]</width>
				<height>$PARAM[ArtHeight]</height>
				<control type="image">
					<width>$PARAM[ArtWidth]</width>
					<height>$PARAM[ArtHeight]</height>
					<texture background="true">$PARAM[ArtFallback]</texture>
					<aspectratio>scale</aspectratio>
				</control>
				<control type="image">
					<centerleft>50%</centerleft>
					<centertop>50%</centertop>
					<width>64</width>
					<height>64</height>
					<include>art_DefaultIcon</include>
				</control>
				<control type="image">
					<width>$PARAM[ArtWidth]</width>
					<height>$PARAM[ArtHeight]</height>
					<texture background="true">$PARAM[ArtType]</texture>
					<aspectratio>scale</aspectratio>
				</control>
				<control type="image">
					<centerleft>50%</centerleft>
					<bottom>10</bottom>
					<width>75%</width>
					<height>50%</height>
					<aspectratio align="center" aligny="bottom">keep</aspectratio>
					<visible>$PARAM[ShowLogo]</visible>
					<include>art_Logo</include>
				</control>
			</control>
		</definition>
	</include>

	<include name="view_List-Focus">
		<param name="Width" default="620"/>
		<param name="Height" default="40"/>
		<definition>
			<control type="image">
				<width>$PARAM[Width]</width>
				<height>$PARAM[Height]</height>
				<texture>views/focus.png</texture>
				<visible>![ControlGroup(6000).HasFocus | ControlGroup(4000).HasFocus | Control.HasFocus(70) | Control.HasFocus(99)]</visible>
			</control>
		</definition>
	</include>

	 <variable name="view:List.TextColor">
		<value condition="![ControlGroup(6000).HasFocus | ControlGroup(4000).HasFocus | Control.HasFocus(70) | Control.HasFocus(99)]">Color4</value>
		<value>Color1</value>
	</variable>
	<variable name="view:List.TextColor2">
		<value condition="![ControlGroup(6000).HasFocus | ControlGroup(4000).HasFocus | Control.HasFocus(70) | Control.HasFocus(99)]">Color4</value>
		<value>Color2</value>
	</variable>

	<include name="view_List">
		<control type="list" id="50">
			<viewtype label="535">list</viewtype> 	
			<left>30</left>
			<top>90</top>
			<width>620</width>
			<height>603</height>
			<onup>50</onup>
			<ondown>50</ondown>
			<onleft>6000</onleft>
			<onright condition="Window.IsActive(videoplaylist) + Player.HasVideo">99</onright>
			<onright condition="Window.IsActive(videoplaylist) + !Player.HasVideo">70</onright>
			<onright condition="Window.IsActive(tvchannels) + Player.HasVideo">99</onright>
			<onright condition="Window.IsActive(tvchannels) + !Player.HasVideo">70</onright>
			<onright>70</onright>
			<pagecontrol>70</pagecontrol>
			<include>toggle_Menu</include>
			<include>global_ListScroll</include>
			<include>transition_Info</include>
			<include>transition_Content</include>
			<itemlayout condition="IsEmpty(Container.FolderPath)" width="620" height="40">		
				<include>view_List-Data.Single</include>
			</itemlayout>
			<focusedlayout condition="IsEmpty(Container.FolderPath)" width="620" height="40">				
				<include>view_List-Focus</include>
				<include name="view_List-Data.Single">
					<param name="TextColor">$VAR[view:List.TextColor]</param>
				</include>
			</focusedlayout>
			<itemlayout condition="SubString(Container.FolderPath,pvr://)" width="620" height="67">		
				<include>view_List-Icon</include>
				<include name="view_List-Data">
					<param name="TextLeft" value="80"/>
					<param name="TextWidth" value="485"/>
				</include>
				<include name="status_List">
					<param name="StatusRight">10</param>
					<param name="StatusTop">19</param>
				</include>
			</itemlayout>
			<focusedlayout condition="SubString(Container.FolderPath,pvr://)" width="620" height="67">				
				<include name="view_List-Focus">
					<param name="Height" value="67"/>
				</include>
				<include>view_List-Icon</include>
				<include name="view_List-Data">
					<param name="TextLeft" value="80"/>
					<param name="TextWidth" value="485"/>
					<param name="TextColor">$VAR[view:List.TextColor]</param>
					<param name="TextColor2">$VAR[view:List.TextColor2]</param>
				</include>
				<include name="status_List">
					<param name="StatusRight">10</param>
					<param name="StatusTop">19</param>
					<param name="StatusColor">$VAR[view:List.TextColor]</param>
				</include>
			</focusedlayout>
			<itemlayout condition="Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos) | Container.Content(artists) | Container.Content(albums) | Container.Contennt(songs) | Container.HasFiles" width="620" height="67">
				<include>view_List-Data</include>
				<include name="status_List">
					<param name="StatusRight">10</param>
					<param name="StatusTop">19</param>
				</include>
			</itemlayout>
			<focusedlayout condition="Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos) | Container.Content(artists) | Container.Content(albums) | Container.Contennt(songs) | Container.HasFiles" width="620" height="67">
				<include name="view_List-Focus">
					<param name="Height" value="67"/>
				</include>
				<include name="view_List-Data">
					<param name="TextColor">$VAR[view:List.TextColor]</param>
					<param name="TextColor2">$VAR[view:List.TextColor2]</param>
				</include>
				<include name="status_List">
					<param name="StatusRight">10</param>
					<param name="StatusTop">19</param>
					<param name="StatusColor">$VAR[view:List.TextColor]</param>
				</include>
			</focusedlayout>
		</control>
	</include>

