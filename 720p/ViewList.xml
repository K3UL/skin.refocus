<includes>
	<include name="view_List">
		<control type="list" id="50">
			<viewtype label="535">list</viewtype> 	
			<posy>90</posy>
			<width>625</width>
			<height>612</height>
			<onup>50</onup>
			<ondown>50</ondown>
			<onleft>6000</onleft>
			<onright>70</onright>
			<pagecontrol>70</pagecontrol>
			<preloaditems>4</preloaditems>
			<scrolltime>150</scrolltime>
			<orientation>vertical</orientation>
			<include>transition_Container</include>
			<include>infoDialogHide</include>
			<include>toggle_Menu</include>
			<!--
			<scrollcorrection>false</scrollcorrection>
			<scrolltime tween="Cubic" easing="InOut">500</scrolltime>
			-->
			<!--
			<focusposition>3</focusposition>
			<movement>3</movement>
			-->	
			<include condition="Window.IsVisible(videolibrary)">ListVideo</include>
			<include condition="Window.IsVisible(musiclibrary) | Window.IsActive(501)">ListMusic</include>
			<include condition="Window.IsVisible(videoplaylist)">ListVideoPlaylist</include>
			<include condition="Window.IsVisible(musicplaylist)">ListMusicPlaylist</include>
			<include condition="Window.IsVisible(pictures)">ListPictures</include>
			<include condition="Window.IsVisible(addonbrowser) | Window.IsVisible(programs)">ListAddons</include>	
		</control>
	</include>
		
	<include name="ListVideo">
		<itemlayout condition="IsEmpty(Container.FolderPath) | ![stringcompare(Container.FolderPath,addons://sources/video/) | Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos)]" width="625" height="40">
