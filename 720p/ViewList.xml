							<texture background="true">thumbs/default_Portrait.png</texture>
							<aspectratio>stretch</aspectratio>
							<fadetime>fadetime_Thumb</fadetime>
						</control>
						<control type="image">
							<left>139</left>
							<top>236</top>
							<width>128</width>
							<height>128</height>
							<include>art_DefaultIcon</include>
							<fadetime>fadetime_Thumb</fadetime>
						</control>
