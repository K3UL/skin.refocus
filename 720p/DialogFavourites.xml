<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
	<defaultcontrol always="true">450</defaultcontrol>
	<include>transition_Dialog</include>
	<coordinates>
		<system>1</system>
		<left>340</left>
		<top>80</top>
	</coordinates>
	<controls>
		<control type="group">
			<include name="dialog_WindowCommons">
				<param name="DialogWidth" value="600" />
				<param name="DialogHeight" value="560" />
				<param name="DialogHeaderName" value="$LOCALIZE[1036]" />
				<param name="DialogHeaderID" value="1" />
				<param name="DialogSeparator" value="true" />
			</include>
			<control type="list" id="450">
				<left>4</left>
				<top>60</top>
				<width>592</width>
				<height>440</height>
				<onup>450</onup>
				<ondown>450</ondown>
				<onright>noop</onright>
				<include>global_ListScroll</include>
				<itemlayout height="40">
					<control type="image">
						<width>592</width>
						<include>dialog_CommonList</include>
					</control>
					<control type="label">
						<width>562</width>
						<label>$INFO[ListItem.Label]</label>
						<include>dialog_CommonListItem</include>
					</control>
				</itemlayout>
				<focusedlayout height="40">
					<control type="image">
						<width>592</width>
						<include>dialog_CommonList</include>
						<visible>!Control.HasFocus(450)</visible>
					</control>
					<control type="image">
						<width>592</width>
						<height>40</height>
						<include>dialog_CommonListFocus</include>
						<visible>Control.HasFocus(450)</visible>
					</control>
					<control type="label">
						<width>562</width>
						<label>$INFO[ListItem.Label]</label>
						<include>dialog_CommonListItemFocus</include>
					</control>
				</focusedlayout>
			</control>
			<control type="label">
				<top>507</top>
				<width>600</width>
				<label>$INFO[Container(450).CurrentItem]/$INFO[Container(450).NumItems]</label>
				<align>center</align>
				<include>dialog_ControlCommons</include>
			</control>
		</control>
	</controls>
</window>